package com.saar;

public class Main {

    public static void main(String[] args) {

        int firstNumber = 100;
        int secondNumber = 9;
        int big = firstNumber;
        int small = secondNumber;

        if(secondNumber > firstNumber) {
            big = secondNumber;
            small = firstNumber;
        }

        System.out.println(multi(big, small));
        System.out.println(quatient(big, small));
        System.out.println(remainder(big, small));
    }

    public static int multi(int x, int y) {

        int result = x;

        for(int i = 1; i < y; i++) {
            result += x;
        }

        return result;

    }

    public static int quatient(int x, int y) {

        int result = 0;

        for(int i = y; i <= x; i += y) {
            result++;
        }

        return result;

    }

    public static int remainder(int x, int y) {

        int a = 0;
        int b = 0;
        int result = 0;

        for(int i = y; i < x; i += y) {
            a++;
        }
        for(int i = 1; i <= a; i ++) {
            b += y;
        }
        for(int i = b; i < x; i++) {
            result++;
        }

        return result;
    }

}
